	"item": [
		{
			"name": "loginAdmin",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Captura la respuesta de la API\r",
							"let response = pm.response.json();\r",
							"\r",
							"// Asigna el token a una variable global\r",
							"pm.globals.set(\"authToken\", response.token);\r",
							"\r",
							"// También puedes guardarlo como variable de entorno en Postman\r",
							"pm.environment.set(\"authToken\", response.token);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": []
			},
			"response": []
